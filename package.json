{
  "name": "merge-packages",
  "version": "0.1.0",
  "description": "Very intelligently merge `package.json` files. 2021 UP_TO_DATE VERSION OF THIS TOOL.",
  "keywords": [
    "package.json",
    "merge package",
    "merge-package",
    "package-merge",
    "package merge",
    "merge json"
  ],
  "main": "dist/index.js",
  "files": [
    "dist"
  ],
  "bin": "dist/cli.js",
  "scripts": {
    "start": "",
    "prebuild": "npm run clean",
    "build": "babel src -d dist",
